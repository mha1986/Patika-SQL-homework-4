--SQL Homework 4 Question 1
SELECT DISTINCT replacement_cost
FROM film;

--SQL Homework 4 Question 2
SELECT COUNT(DISTINCT replacement_cost)
FROM film;

--SQL Homework 4 Question 3
SELECT COUNT(title)
FROM film
WHERE title LIKE'T%' AND rating='G';

--SQL Homework 4 Question 4
SELECT COUNT(country)
FROM country
WHERE country LIKE'_____';

--SQL Homework 4 Question 5
SELECT COUNT(city)
FROM city
WHERE city ILIKE'____r';
